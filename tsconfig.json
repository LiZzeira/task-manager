{
  "compilerOptions": {
    "resolveJsonModule": true,
    "strictPropertyInitialization": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "outDir": "./dist",
    "module": "commonjs",
    "target": "es6",
    "esModuleInterop": true,
    "allowJs": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "sourceMap": true
  },
  "exclude":[
    "./node_modules",
    "./dist"
  ]
}